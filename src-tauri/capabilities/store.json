{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "store",
  "description": "Store Plugin 权限 - 持久化数据存储",
  "windows": ["main"],
  "permissions": [
    "store:default",
    "store:allow-get",
    "store:allow-set",
    "store:allow-has",
    "store:allow-delete",
    "store:allow-clear",
    "store:allow-keys",
    "store:allow-values",
    "store:allow-entries",
    "store:allow-length",
    "store:allow-load",
    "store:allow-save"
  ]
}

